@using System.Web.Mvc.Html
@using HtmlTableHelper
@model HtmlTableHelper.ViewModel.TableViewModel

<table>
    <thead>
        <tr>
            @foreach (var col in Model.RenamedHeader)
            {
                <th>
                    @col
                </th>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model.Rows)
        {
            <tr>
                @for (int i = 0; i < Model.Header.Count; i++)
                {
                    <td>
                        @row[i]
                    </td>
                }
            </tr>
        }
    </tbody>
</table>
